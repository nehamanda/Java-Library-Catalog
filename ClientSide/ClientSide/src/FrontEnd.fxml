<AnchorPane prefHeight="615.0" prefWidth="761.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ClientListener">
    <ImageView fitHeight="615.0" fitWidth="761.0" pickOnBounds="true">
        <image>
            <Image url="@Images/shelves.jpg" />
        </image></ImageView>
    <Label layoutX="210.0" layoutY="167.0" prefHeight="48.0" prefWidth="349.0" text="Manda Public Library" textFill="#b17575">
      <font>
         <Font name="Headline R" size="27.0" />
      </font></Label>
    <TextField fx:id="usernameField" layoutX="209.0" layoutY="236.0" prefHeight="26.0" prefWidth="350.0" promptText="Enter username">
